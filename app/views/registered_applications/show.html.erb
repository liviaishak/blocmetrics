<div class="container">
  <div class="row">
    <div class="col-md-8">
      <div>
        <h3><%= @registered_application.name %></h3>
      </div>

      <div class="row">
       <h4><%= @registered_application.url %></h4>
       <p><%= @events %></p>
      </div>
    </div>

    <div class="col-md-4">
      <%= link_to "Back to index", registered_applications_path, class: 'btn btn-primary' %>
      <%= link_to "Edit", edit_registered_application_path(@registered_application), class: 'btn btn-success' %>
      <%= link_to "Delete", @registered_application, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this post?'} %>
    </div>
  </div>
</div>
